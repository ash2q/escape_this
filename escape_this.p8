--controls



function player_control()
	l=0
	r=1
	u=2
	d=3
	bx=5
	bz=4
	x=sb_x
	y=sb_y
	if btn(l) then
		sb_x-=sb_speed
	end
	if btn(r) then
		sb_x+=sb_speed
	end
	if btn(u) then
		sb_y-=sb_speed
	end
	if btn(d) then
		sb_y+=sb_speed
	end
	if sb_y != y or 
			 sb_x != x then
			 sb_moved=true
	else
			sb_moved=false
	end
	--keep from going out of bounds
	m=midline
	if not in_stage then
		m=1
	end
	if sb_y<m or
				sb_y>bot_border then
		sb_y=y
	end
	if sb_x<l_border or
				sb_x>r_border then
		sb_x=x
	end
end


function gun_control()
	bx=5
	bz=4
	x=sb_x
	y=sb_y
	spec=x_gun.spec
	if btn(bx) then
		--do rate of fire
		if x_gun.r>=x_gun.spec.rate then
			x_gun.r=0
		else
			x_gun.r+=sb_rate_mod
		end
		if x_gun.r==0 then
			--shoot bullet now
			if x_gun.heat>100 then
				spec.shoot_heated()
			else
				spec.shoot()
			end
			x_gun.heat+=
				(x_gun.spec.heat_rate*
				x_gun.heat_mul)*
			 sb_gun_heat_mul
			--x_gun.heat+=spec.heat_rate
			--x_gun.heat*=sb_gun_heat_mul
			if x_gun.heat>x_gun.max_heat
			 then
				x_gun.heat=x_gun.max_heat
				sb_heat+=sb_gun_overheat
			end
		end
	else
		--x_gun.c=0
	end
	x_gun.r=max(x_gun.r,0)
	--z_gun.r=max(z_gun.r,0)
end

