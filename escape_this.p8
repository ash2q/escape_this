--projectiles

bullets={}


function draw_bullets()
	printh(#bullets)
	for b in all(bullets) do
		printh("+")
		printh(#b)
		for k,v in all(b) do
			printh(k)
			printh(v)
		end
		printh("----")
		if b.sprite<0 then
			pset(b.x,b.y,abs(b.sprite))
		else
		 spr(b.x,b.y,b.sprite)
		end
		b.step_fn(b)
		
		if b.friendly then
		--	b.y-=1
		else
		--	b.y+=1
		end
	end
end

function shoot_chain()
	x1=sb_x+3
	y=sb_y
	dmg=x_gun.spec.dmg
	blt=blt_straight(x1,y,dmg)
	add(bullets,blt)
end

function shoot_heated_chain()
	x1=sb_x+3
	y=sb_y
	dmg=x_gun.spec.dmg
	blt=blt_heated_straight(x1,y,
		dmg)
	add(bullets,blt)
end

function blt_straight(x,y,dmg)
	blt={}
	blt.x=x
	blt.y=y
	blt.xend=x
	blt.yend=0
	blt.size=1
	blt.dmg=dmg
	blt.steps=1
	blt.friendly=true
	blt.sprite=-7
	blt.hit_fn=hit_blt
	blt.step_fn=step_straight
	blt.collision_fn=col_pixel
	return blt
end

function blt_heated_straight(x,y,dmg)
	blt=blt_straight(x,y,dmg)
	blt.sprite=-8
	return blt
end

function hit_blt(blt, e)
	e.health-=blt.damage
end

function col_pixel(x1,y1,x2,y2,
			w,h)
			--defaults to 8x8
			w=w or 8
			h=h or 8
--x1,y1 = pixel bullet
--x2,y2 = target sprite to check
--w,h = size of target sprite
end


