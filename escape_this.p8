--step functions

function step_straight(b)
	--modify x,y
	if b.x<b.xend then
		b.x+=1
	elseif b.x>b.xend then
		b.x-=1
	end
	if b.y<b.yend then
		b.y+=1
	elseif b.y>b.yend then
		b.y-=1
	end
	
	return b
end
