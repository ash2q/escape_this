reactor_depth=1
reactor_stages={
	stage_1
}
function reactor()
	if 
		reactor_depth>#reactor_stages
		then
		reactor_stages
			[#reactor_stages]()
	else
		reactor_stages
			[reactor_depth]()
	end
	
end

locations={}

reactor_loc={
	sprite=64,
	launches=reactor,
	mode=stage_mode,
	msg=
		"enter reactor? (❎ for yes)"
}

function add_map_loc(x,y,loc)
	add(locations,{
		x=x,
		y=y,
		loc=loc
	})
end

function build_map()
	add_map_loc(12*8,3*8,
		reactor_loc)
	
end

function check_col(x1,y1,w1,h1,
										x2,y2,w2,h2)
	x_col=false
	if x1<=x2+w2 and x1>x2 then
		x_col=true
	end
	if x1-w1<=x2 and x1+w1>x2 then
		x_col=true
	end
	y_col=false
	if y1<=y2+h2 and y1>y2 then
		y_col=true
	end
	if y1-h1<=y2 and y1+h1>=y2 then
		y_col=true
	end
	
	return x_col and y_col
end

function check_player_choice()
	for l in all(locations) do
		if check_col(sb_x,
							sb_y,
							8,
							8,
							l.x,
							l.y,
							16,
							16) then
			--player chose
			prompt(l.loc)
			
		end
	end
end

function prompt(l)
	prompt_status=3 --waiting
	prompt_msg=l.msg
	in_prompt=true
	prompt_loc=l
	default_pos()
	
end

