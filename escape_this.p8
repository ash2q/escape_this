--drawing

function draw_player()
	x=sb_x
	y=sb_y
	
	anim=sb_anim
	if sb_heated then
		anim=sb_heated_anim
		--particle effects
		for i=0,4 do
			rx=(x-1)+rnd(10)
			ry=(y-2)+rnd(8)
			pset(rx, ry, 8)
		end
	end
	
	if sb_moved then
			spr(anim[
				1+(acnt%#anim)],
				sb_x,sb_y)
	else
			spr(anim[1],
				sb_x,sb_y)
	end
end

function draw_enemies()
	for e in all(enemies) do
		tmp=acnt%#e.e.anim
		spr(e.e.anim[1+tmp],
			e.x, e.y)
	end
end

